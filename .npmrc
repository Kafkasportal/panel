# NPM Configuration
# Suppress deprecated warnings for transitive dependencies
# Note: node-domexception is deprecated but harmless on Node.js 20+
# as DOMException is built-in

# Optional: Uncomment to suppress all deprecated warnings
# warn-deprecated=false

# Use legacy peer deps if needed
legacy-peer-deps=false

# Save exact versions (optional)
save-exact=false

# Package lock file
package-lock=true
